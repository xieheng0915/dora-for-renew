<div class="home_page7" id="inquiry">
  <div class="contact_content">
    <div v-if="!cloudSuccess">
      <div class="large-title">
        <%= __('common.menu.contact') %>
      </div>
      <ajax-form action="deliverContactFormMessage" :syncing.sync="syncing" :cloud-error.sync="cloudError" @submitted="submittedForm()"
        :handle-parsing="handleParsingForm">
        <div class="d-md-flex justify-content-between">
          <div class="form-group">
            <input class="form-control name home_input" autocomplete="off" id="full-name" name="full-name" type="text" :class="[formErrors.fullName ? 'is-invalid' : '']"
              v-model.trim="formData.fullName" placeholder=<%=__( 'nick-name' ) %>>
            <div class="invalid-feedback error" id="err-emailvld" v-if="formErrors.fullName">
              <%= __('contact.error.name') %>
            </div>
          </div>
          <div class="form-group">
            <input class="form-control home_input" autocomplete="off" id="email-address" name="email-address" type="email" :class="[formErrors.emailAddress ? 'is-invalid' : '']"
              v-model.trim="formData.emailAddress" placeholder=<%=__( 'email-example' ) %>>
            <div class="invalid-feedback error" id="err-emailvld" v-if="formErrors.emailAddress">
              <%= __('login.error.emailaddress') %>
            </div>
          </div>
        </div>
        <div>
          <textarea class="form-control w-100 home_textarea" rows="7" id="message" name="message" :class="[formErrors.message ? 'is-invalid' : '']"
            v-model.trim="formData.message" placeholder="<%= __('contact.message') %>"></textarea>
          <div class="invalid-feedback error" id="err-message" v-if="formErrors.message">
            <%- __('contact.error.message') %>
          </div>
        </div>
        <div class="home_botton submit text-right">
          <button type="submit" :syncing="syncing" class="submit-large-button phone-submit-button">
            <%= __('submit') %>
          </button>
        </div>
      </ajax-form>
    </div>

    <div v-if="cloudSuccess">
      <div class="text-center">
        <div class="large-title">
          <%= __('common.menu.contact') %>
        </div>
        <div>
          <%- __('email-sent') %>
        </div>
      </div>
    </div>
  </div>
</div>

